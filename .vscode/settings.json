{
  "cSpell.words": [
    "acro",
    "asynciterable",
    "autodocs",
    "clsx",
    "codegen",
    "commitlint",
    "connectrpc",
    "copyfiles",
    "datasources",
    "deepmerge",
    "fontsource",
    "gomavlib",
    "isbot",
    "manypkg",
    "mapbox",
    "maptiler",
    "mavlink",
    "Naresh",
    "nareshbhatia",
    "nextjs",
    "remrssi",
    "rssi",
    "SITL",
    "statustext",
    "tailwindcss",
    "tanstack",
    "testid",
    "tsup",
    "turborepo",
    "typecheck",
    "vite"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.prettier": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.indentSize": 2,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.insertFinalNewline": true
}
